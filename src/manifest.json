{
    "manifest_version": 2,
    "name": "Gainsight fix for Firefox",
    "version": "1.0.5",
    "description": "Fix Gainsight horizontal scrollbar issue in Firefox",
    "content_scripts": [
        {
            "matches": ["*://ibm.gainsightcloud.com/*"],
            "js": [ "main.js" ]
        }
    ],


    "browser_specific_settings": {
        "gecko": {
            "id": "gsffix@yamamoto.ar"
        }
    }
}
